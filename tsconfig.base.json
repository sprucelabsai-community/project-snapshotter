{
	"compilerOptions": {
		"skipLibCheck": true,
		"module": "ESNext",
		"esModuleInterop": true,
		"target": "ES2022",
		"lib": ["DOM", "ES2022"],
		"declaration": true,
		"noImplicitAny": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"strict": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"experimentalDecorators": true
	}
}
